{
  "name": "base",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:base": "NODE_ENV=production nest build base",
    "build:ai": "NODE_ENV=production nest build ai",
    "build": "NODE_ENV=production conc pnpm:build:*",
    "dev:base": "NODE_ENV=development nest start base --watch",
    "dev:ai": "NODE_ENV=development nest start ai --watch",
    "dev": "NODE_ENV=development conc --kill-others pnpm:dev:*",
    "deploy": "pm2 startOrRestart process.json",
    "lint": "eslint '{apps,packages,libs}/**/*.ts' --fix",
    "format": "prettier --write 'apps/**/*.ts' 'libs/**/*.ts'",
    "update-api": ". update-api.sh",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/base/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-plus/rabbitmq": "^1.4.4",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.13.0",
    "@zilliz/milvus2-sdk-node": "^2.2.7",
    "axios": "^1.3.6",
    "cache-manager": "^5.2.1",
    "cache-manager-redis-yet": "^4.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^8.0.1",
    "cypher-query-builder": "^6.0.4",
    "hasha": "^5.2.2",
    "js-levenshtein": "^1.1.6",
    "langchain": "^0.0.64",
    "mime-types": "^2.1.35",
    "nestjs-prisma": "^0.20.0",
    "nestjs-request-context": "^2.1.0",
    "prisma": "^4.13.0",
    "redis": "^4.6.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/js-levenshtein": "^1.1.1",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "config": "workspace:*",
    "eslint": "^8.0.1",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.3.1",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/env(|/.*)$": "<rootDir>/libs/env/src/$1",
      "^@app/rmq(|/.*)$": "<rootDir>/libs/rmq/src/$1",
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@app/graph(|/.*)$": "<rootDir>/libs/graph/src/$1"
    }
  }
}